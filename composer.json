{
    "name": "milyoona/microservice-monitor",
    "description": "Laravel Nova tool for microservice monitoring.",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug": "^2.2",
        "php-http/message": "^1.11",
        "supervisorphp/supervisor": "^4.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "lstrojny/fxmlrpc": "^0.20.0"
    },
    "autoload": {
        "psr-4": {
            "Milyoona\\MicroserviceMonitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Milyoona\\MicroserviceMonitor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
